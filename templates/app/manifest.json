{
  "name": "${manifest.name}",
  "short_name": "${manifest.shortName}",
  "start_url": "/",
  "theme_color": "${manifest.themeColor}",
  "background_color": "${manifest.backgroundColor}",
  "display": "standalone",
  "icons": [
    {
      "src": "/favicon.ico",
      "sizes": "any",
      "type": "image/x-icon"
    },
    {
      "src": "/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
